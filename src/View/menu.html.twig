<section id="menu" class="fondMenu">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <h1 class="titleMenu">
                    <img src="pictures/couverts.png" class="logoMenu" alt="Couverts">
                    <span class="yellow">LA CARTE</span>
                </h1>
                <span class="titleStars">********************************************</span>
                <br/>
                <span class="titlePhrase">{{ displayTexts.4 | raw }}</span>
            </div>
            {% for i in range(5, 8) %}
                <div class="col-xs-4 col-xs-offset-1 menuMargin">
                    <div class="vignettes">
                        <div class="col-xs-12">
                            <h3>{{ displayTexts[i] | raw }}</h3>
                        </div>
                        <img src="pictures/{{ displayPicturesLocalSrc[i] }}" alt="{{ displayPictures[i] }}"/>
                    </div>
                </div>
            {% endfor %}
        </div><!-- row -->
    </div><!-- container -->
</section>
